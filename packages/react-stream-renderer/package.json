{
  "name": "react-stream-renderer",
  "version": "0.2.0",
  "description": "",
  "license": "MIT",
  "repository": "https://github.com/zamotany/react-stream-renderer",
  "private": false,
  "author": {
    "name": "Paweł Trysła",
    "email": "pawel.trysla@gmail.com"
  },
  "files": [
    "/build",
    "index.js"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build:transpile": "babel src --out-dir build --ignore **/__tests__/**",
    "build:clean": "rimraf build",
    "build:watch": "yarn build:transpile --watch",
    "build": "yarn build:clean && yarn build:transpile",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "dependencies": {
    "ansi-parser": "^3.2.8",
    "ansi-styles": "^3.2.0",
    "astral-regex": "^1.0.0",
    "chalk": "^2.1.0",
    "cli-spinners": "^1.1.0",
    "fast-memoize": "^2.2.8",
    "fbjs": "^0.8.16",
    "is-fullwidth-code-point": "^2.0.0",
    "prop-types": "^15.6.0",
    "react-reconciler": "0.3.0-beta.1",
    "strip-ansi": "^4.0.0"
  },
  "devDependencies": {
    "react": "^16.2.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "flow-react-proptypes",
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "eslintConfig": {
    "extends": "callstack-io"
  }
}

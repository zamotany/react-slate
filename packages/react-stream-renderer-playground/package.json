{
  "name": "react-stream-renderer-playground",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "repository": "https://github.com/zamotany/react-stream-renderer",
  "private": true,
  "author": {
    "name": "Paweł Trysła",
    "email": "pawel.trysla@gmail.com"
  },
  "files": [
    "/build",
    "index.js"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "node ./build",
    "build:transpile": "babel src --out-dir build --ignore **/__tests__/**",
    "build:clean": "rimraf build",
    "build:watch": "yarn build:transpile --watch",
    "build": "yarn build:clean && yarn build:transpile",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-stream-renderer": "^0.2.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "flow-react-proptypes",
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "eslintConfig": {
    "extends": "callstack-io"
  }
}
